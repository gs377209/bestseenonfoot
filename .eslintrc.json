{
  "extends": ["eslint:recommended", "next/core-web-vitals", "prettier"],
  "rules": {
    "no-duplicate-imports": "warn",
    "no-useless-rename": "warn",
    "sort-imports": "warn",
    "sort-keys": "warn",
    "sort-vars": "warn",
    "no-unused-private-class-members": "warn",
    "no-unused-expressions": "warn"
  }
}
